{
  "displayName": "Homebridge MELCloud",
  "name": "homebridge-melcloud-ts",
  "version": "0.5.0-rc5",
  "description": "Homebridge plugin for MELCloud.",
  "main": "dist/index.js",
  "files": [
    "dist/*.js",
    "dist/**/*.js",
    "config.schema.json"
  ],
  "scripts": {
    "clean": "rimraf ./dist ./coverage",
    "build": "rimraf ./dist ./coverage && tsc",
    "lint": "eslint src/**/*.{ts,tsx} test/**/*.{ts,tsx} --fix",
    "lint-check": "eslint src/**/*.{ts,tsx} test/**/*.{ts,tsx}",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint-check",
    "postpublish": "npm run clean",
    "publish-next": "npm publish . --tag next",
    "watch": "npm run clean && tsc --watch",
    "test": "jest --verbose"
  },
  "engines": {
    "node": ">=16",
    "homebridge": ">=1.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Dids/homebridge-melcloud-ts.git"
  },
  "keywords": [
    "homebridge-plugin",
    "Mitsubishi",
    "Melcloud",
    "homekit",
    "Siri",
    "ts",
    "TypeScript"
  ],
  "author": "ilcato",
  "contributors": [
    "Dids"
  ],
  "maintainers": [
    "Dids"
  ],
  "license": "GPL2",
  "bugs": {
    "url": "https://github.com/Dids/homebridge-melcloud-ts/issues"
  },
  "homepage": "https://github.com/Dids/homebridge-melcloud-ts",
  "dependencies": {
    "async-mutex": "^0.3.2",
    "lodash": "^4.17.21",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.2.3",
    "node-persist": "^3.1.0",
    "object-hash": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.25",
    "@types/node-cache": "^4.2.5",
    "@types/node-fetch": "^2.6.1",
    "@types/node-persist": "^3.1.2",
    "@types/object-hash": "^2.2.1",
    "@types/request-promise-native": "^1.0.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.13.0",
    "homebridge": "^1.4.0",
    "intermock": "^0.2.5",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
