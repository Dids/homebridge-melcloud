{
  "displayName": "Homebridge MELCloud",
  "name": "homebridge-melcloud-ts",
  "version": "0.6.0-rc4",
  "publishConfig": {
    "tag": "rc"
  },
  "description": "Homebridge plugin for MELCloud.",
  "main": "dist/index.js",
  "files": [
    "dist/*.js",
    "dist/**/*.js",
    "config.schema.json"
  ],
  "scripts": {
    "clean": "rimraf ./dist ./.coverage",
    "build": "rimraf ./dist ./.coverage && tsc -p tsconfig.prod.json && tsc-alias",
    "build-dev": "rimraf ./dist ./.coverage && tsc -p tsconfig.json && tsc-alias",
    "lint": "eslint src/**/*.ts test/**/*.ts --fix",
    "lint-check": "eslint src/**/*.ts test/**/*.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint-check",
    "postpublish": "npm run clean",
    "publish-next": "npm publish . --tag next",
    "watch": "npm run clean && tsc --watch",
    "test": "jest --verbose"
  },
  "engines": {
    "node": ">=18",
    "homebridge": ">=1.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Dids/homebridge-melcloud-ts.git"
  },
  "keywords": [
    "homebridge-plugin",
    "Mitsubishi",
    "Electric",
    "AC",
    "Air Conditioner",
    "Heating",
    "Cooling",
    "Thermostat",
    "Air Conditioner",
    "Melcloud",
    "homekit",
    "Siri",
    "ts",
    "TypeScript"
  ],
  "author": "ilcato",
  "contributors": [
    "Dids"
  ],
  "maintainers": [
    "Dids"
  ],
  "license": "GPL2",
  "bugs": {
    "url": "https://github.com/Dids/homebridge-melcloud-ts/issues"
  },
  "homepage": "https://github.com/Dids/homebridge-melcloud-ts",
  "dependencies": {
    "async-mutex": "^0.4.0",
    "lodash": "^4.17.21",
    "node-cache": "^5.1.2",
    "node-fetch-native": "^1.4.0",
    "node-persist": "^3.1.3",
    "object-hash": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/lodash": "^4.14.198",
    "@types/node": "^18.17.1",
    "@types/node-cache": "^4.2.5",
    "@types/node-fetch": "^2.6.5",
    "@types/node-persist": "^3.1.4",
    "@types/object-hash": "^3.0.4",
    "@types/request-promise-native": "^1.0.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "dotenv": "^16.3.1",
    "eslint": "^8.49.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "homebridge": "^1.6.1",
    "intermock": "^0.2.5",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-extended": "^4.0.1",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.2.2",
    "with-local-tmp-dir": "^5.0.1"
  }
}
